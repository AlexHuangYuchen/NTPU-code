#include <stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    if(n>0){
        printf("%d是正數\n",n);
        if(n<=10000){
            printf("A");
        }
        else{
            printf("B");
        }
    }
    else if(n<0){
        printf("%d是負數\n",n);
        if(n<=-10000){
            printf("C");
        }
        else{
            printf("D");
        }
    }
    else {
        printf("0不是正數也不是負數");
    }
}
