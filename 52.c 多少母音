#include<stdio.h>
#include<string.h>
int main(){
    char l[1000];
    int sum=0;
    scanf("%s",&l);
    //printf("%d\n",strlen(l));
    //printf("%s\n",l);
    int ascii;
    for(int i=0;i<strlen(l);i++){
        ascii=l[i];
        if(ascii==65 || ascii==97 || ascii==69 || ascii==101 || ascii==73 || ascii==105 || ascii==79 || ascii==111 || ascii==85 || ascii==117){
            sum++;
        }
    }
    printf("%d",sum);
}
